<resources>

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v10/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!-- API 14 theme customizations can go here. -->
        <item name="ic_action_logout">@drawable/ic_action_logout_dark</item>
        <item name="ic_action_call">@drawable/ic_action_call_light</item>
        <item name="ic_local">@drawable/ic_local_light</item>
        <item name="ic_omni">@drawable/ic_omni_light</item>
    </style>

</resources>
