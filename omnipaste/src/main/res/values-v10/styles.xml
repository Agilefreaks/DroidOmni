<resources>

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!-- API 10 theme customizations can go here. -->
        <item name="ic_action_logout">@drawable/ic_action_logout_light</item>
        <item name="ic_action_call">@drawable/ic_action_call_dark</item>
        <item name="ic_local">@drawable/ic_local_dark</item>
        <item name="ic_omni">@drawable/ic_omni_dark</item>
    </style>

</resources>
